// Module 7- Lab 1- Student structures 

#include <iostream>
#include <string>
using namespace std;

struct Student {
string studentName;
double gpa;
string major;
};

int main(){
    Student student1;
    student1.studentName = "Kalani";
    student1.gpa = 3.5;
    student1.major = "Computer Science";
    cout << "Student Name: " << student1.studentName << endl;
    cout << "GPA: " << student1.gpa << endl;
    cout << "Major: " << student1.major << endl;

    return 0;
}
