#include <iostream>
using namespace std;

void add(int pNumOne, int pNumTwo);

int main() 
{
int numOne = 6;
  int numTwo = 15;

add(numOne, numTwo);
  
  return 0;
}

void add(int pNumOne, int pNumTwo)
{
  int result;
  result = pNumOne + pNumTwo;
 cout << "The result is: " << result << endl;
//cout << "The result is: " << pNumOne + pNumTwo << endl;
}
