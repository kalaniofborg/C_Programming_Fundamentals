// Practice with Structures

#include <iostream>
#include <string>
using namespace std;

struct DreamCar {
  string make;
  string model;
  double price;
  int year;
};
void PrintDreamCar(DreamCar myCar);

int main() {

  DreamCar myCar;
  myCar.make = "Audi";
  myCar.model = "A6 Quattro";
  myCar.price = 7000;
  myCar.year = 1999;

  PrintDreamCar(myCar);

  return 0;
}
void PrintDreamCar(DreamCar myCar) {

  cout << "Model: " << myCar.model << endl;
  cout << "Make: " << myCar.make << endl;
  cout << "Price: " << myCar.price << endl;
  cout << "Year: " << myCar.year << endl;
}
